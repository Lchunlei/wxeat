page{
    height: 100%;
}
.container{
    background-color: #ffffff;
    justify-content: initial;
}
.title {
  color: #333;
  font-weight:700;
  font-size: 32rpx;
  margin-bottom: 10rpx;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  padding-top: 20rpx;
}

.text {
  font-size: 30rpx;
  width: 95%;
  margin: 0 auto;
}
.shoukuan {
  height: 80%;
  text-align: center;
}
.tixian-bg,.tixian-img{
  width: 100%;
  height: 500rpx;
}
.tixian-name{
  position:absolute;
  width:100%;
  top:180rpx;
  color:#fff;
}
.tixian-title{
  display:block;
  font-size:48rpx;
  margin-top:5px;
  text-align: center;
}
.tixian-name .money{
  display:block;
  font-size:35rpx;
  margin-top:5px;
  margin-left:80px;
}
.tixian-name .tips{
  font-size:30rpx;
  margin-top:30rpx;
  display:block;
  margin-left:80px;
}
.row-wrap{
    width:90%;
    line-height:88rpx;
    font-size:28rpx;
    margin-left:20px;
}
.row-wrap .label{
    width:100%;
    color:#666;
    font-size:26rpx;
}
.row-wrap .label-right{
    height:88rpx;
    line-height:88rpx;
    border-bottom:1px solid #f5f5f5;
}
.row-wrap .label-right input{
    height:100%;
    font-size:27rpx;
    float:left;
    margin:-5px 0;
    width:80%;
    padding:5px 0;
    color:#333;
}